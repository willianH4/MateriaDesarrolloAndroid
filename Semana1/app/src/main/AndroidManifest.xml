<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.willianhdz.semana1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/simbolo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/simbolo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Ejemplo1"
            android:parentActivityName=".MainActivity"
            android:label="Primer proyecto">
            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name=".Ejemplo2"
            android:parentActivityName=".MainActivity"
            android:label="Ejemplo 2">
            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>